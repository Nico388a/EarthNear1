@page
@model EarthNear1.Pages.Shifts.GetAllShiftsModel
@{
    ViewData["Title"] = "GetAllShifts";
}
<h1>VagtTider</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Vagt Id
            </th>
            <th>
                Vagt fra
            </th>
            <th>
                Vagt til
            </th>
            <th>
                Vagt på døgnet
            </th>
        </tr>
    </thead>
    <tbody>
    @foreach (var item in Model.Shifts)
    {
        <tr>
            <td>
                @item.ShiftId
            </td>
            <td>
                @item.DateFrom
            </td>
            <td>
                @item.DateTo
            </td>
            <td>
                @item.Description
            </td>

        </tr>
    }
    </tbody>
</table>
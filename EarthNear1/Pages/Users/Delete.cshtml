@page
@model EarthNear1.Pages.Users.DeleteModel
@{
    ViewData["Title"] = "Slet arbejder";
}

<h1>Slet profil</h1>

<form method="post">
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Navn:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.User.Name)
        </dd>
        <dt class="col-sm-2">
            Efternavn:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.User.AfterName)
        </dd>
        <dt class="col-sm-2">
            tlf:
        </dt>
        
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.User.PhoneNumber)
        </dd>
        
        <dt class="col-sm-2">
            Email:
        </dt>
        
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.User.Email)
        </dd>
        <dt 
            class="col-sm-2">Password</dt>
        <dd class="form-group">
            @Html.DisplayFor(model=>model.User.Password)
        </dd>
    </dl>

    <input type="submit" value="Slet" class="btn btn-danger" />
    <a asp-page="./GetAllUsers">Tilbage til medlemslisten</a>
</form>